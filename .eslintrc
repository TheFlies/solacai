{
  "parserOptions": {
    "sourceType": "module"
  },
  "rules": {
    "spaced-comment": 1,
    "arrow-body-style": 1,
    "no-unused-vars": 1,
    "no-unused-expressions": 0,
    "comma-dangle": [2, "never"],
    "comma-spacing": [2, { "before": false, "after": true }],
    "comma-style": [2, "last"],
    "object-curly-spacing": [2, "never"],
    "prefer-template": 0,
    "indent": [2, 2, {"SwitchCase": 1}],
    "quotes": [
      2,
      "single"
    ],
    "semi": [
      2,
      "always"
    ],
    "new-cap": [2, {
      "capIsNew" : false
    }],
    "no-use-before-define": 0,
    "no-multi-spaces" : 0,
    "no-reserved-keys": 0,
    "no-wrap-func": 0,
    "spaced-line-comment": 0,
    "func-names": 0,
    "id-length": 0
  },
  "env": {
    "es6": true,
    "node": true
  },
  "ecmaFeatures": {
    "experimentalObjectRestSpread": true,
    "modules": true
  },
  "globals": {
    "__LANG__": true,
    "__TESTING__": true,
    "__DEVTOOLS__": true,
    "__DEVELOPMENT__": true,
    "Observable": true,
    "Subscription": true,
    "__": true,
    "importScripts": true
  }
}